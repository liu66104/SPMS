    <item>
        <shape android:shape="oval" >
            <solid android:color="#4e4e4e" />
        </shape>
    </item>
    <item
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0.5dp">
        <shape android:shape="oval">
            <solid android:color="#e0CFEBF6" />
