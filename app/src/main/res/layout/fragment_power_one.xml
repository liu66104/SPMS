<?xml version="1.0" encoding="utf-8"?>
<com.example.administrator.spms.refresh.PullToRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/refresh_power_one"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    android:orientation="vertical">

    <include layout="@layout/allview_refresh_head" />
    <!-- 支持所有实现Pullable接口的View -->

    <com.example.administrator.spms.refresh.PullableScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp">

                <View
                    android:id="@+id/tag_power_one"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/tv_name_power_one"
                    android:layout_marginTop="20dp"
                    android:background="@color/color_operate">

                </View>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tag_power_one"
                    android:layout_marginTop="@dimen/ten"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_power_one"
                        android:layout_width="0dp"
                        android:layout_height="120dp"
                        android:layout_marginLeft="@dimen/twelve"
                        android:layout_weight="2.0"
                        android:scaleType="fitXY" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/fifteen"
                        android:layout_toRightOf="@+id/iv_power_one"
                        android:layout_weight="2"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_power_one_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/ten"
                            android:text="" />

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_name_power_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="22sp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tv_name_power_one"
                android:layout_marginTop="@dimen/ten"
                android:background="@color/color_operate"></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                     <RelativeLayout
                         android:id="@+id/rl_power1"
                         android:layout_width="match_parent"
                         android:layout_height="40dp">
                         <TextView
                             android:id="@+id/tv_power_one_tag"
                             android:layout_width="150dp"
                             android:layout_height="wrap_content"
                             android:layout_marginLeft="@dimen/five"
                             android:layout_centerVertical="true"
                             android:layout_toRightOf="@+id/iv_power_one_tag"
                             android:text=""
                             android:textSize="14sp" />


                         <ImageView
                             android:id="@+id/iv_power_one_tag"
                             android:layout_width="30dp"
                             android:layout_height="30dp"
                             android:layout_centerVertical="true"
                             android:layout_marginLeft="@dimen/twelve"
                             android:src="@mipmap/rongliang"
                             android:visibility="gone" />
                         <RelativeLayout
                             android:layout_width="45dp"
                             android:layout_height="wrap_content"
                             android:layout_centerVertical="true"
                             android:layout_alignParentRight="true"
                             android:layout_marginRight="10dp"
                             >
                             <TextView
                                 android:id="@+id/tv_tag0_power"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:text="" />
                            </RelativeLayout>



                         <TextView
                             android:id="@+id/tv_num_power"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:layout_alignParentRight="true"
                             android:layout_marginRight="60dp"
                             android:layout_centerVertical="true"
                             android:text=""
                             android:textSize="@dimen/text_sixteen" />

                         <View
                             android:id="@+id/tag_power_one"
                             android:layout_width="match_parent"
                             android:layout_height="1dp"
                             android:layout_marginTop="39dp"
                             android:background="@color/color_operate">

                         </View>
                     </RelativeLayout>

                      <RelativeLayout
                          android:layout_below="@+id/rl_power1"
                          android:id="@+id/rl_power2"
                          android:layout_width="match_parent"
                          android:layout_height="40dp">
                          <TextView
                              android:id="@+id/tv_power_one_tag2"
                              android:layout_width="150dp"
                              android:layout_height="wrap_content"
                              android:layout_centerVertical="true"
                              android:layout_marginLeft="@dimen/five"
                              android:layout_toRightOf="@+id/iv_power_one_tag2"
                              android:text=""
                              android:textSize="14sp" />


                          <ImageView
                              android:id="@+id/iv_power_one_tag2"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_centerVertical="true"
                              android:layout_marginLeft="@dimen/twelve"
                              android:src="@mipmap/fadianliang"
                              android:visibility="gone" />


                          <RelativeLayout
                              android:layout_centerVertical="true"
                              android:layout_alignParentRight="true"
                              android:layout_marginRight="10dp"
                              android:layout_width="45dp"
                              android:layout_height="wrap_content">
                              <TextView
                                  android:id="@+id/tv_tag_power"
                                  android:layout_width="wrap_content"
                                  android:layout_height="wrap_content"
                                  android:text="" />
                          </RelativeLayout>



                          <TextView
                              android:layout_centerVertical="true"
                              android:id="@+id/tv_num2_power"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_alignParentRight="true"
                              android:layout_marginRight="60dp"
                              android:text=""
                              android:textSize="@dimen/text_sixteen" />


                          <View
                              android:id="@+id/tag2_power_one"
                              android:layout_width="match_parent"
                              android:layout_height="1dp"
                              android:layout_marginTop="39dp"
                              android:background="@color/color_operate">

                          </View>
                      </RelativeLayout>

                      <RelativeLayout
                          android:id="@+id/rl_power3"
                          android:layout_below="@+id/rl_power2"
                          android:layout_width="match_parent"
                          android:layout_height="40dp">
                          <TextView
                              android:id="@+id/tv_power_one_tag3"
                              android:layout_width="150dp"
                              android:layout_height="wrap_content"
                              android:layout_centerVertical="true"
                              android:layout_marginLeft="@dimen/five"
                              android:layout_toRightOf="@+id/iv_power_one_tag3"
                              android:text=""
                              android:textSize="14sp" />


                          <ImageView
                              android:id="@+id/iv_power_one_tag3"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_centerVertical="true"
                              android:layout_marginLeft="@dimen/twelve"
                              android:src="@mipmap/fadianliang"
                              android:visibility="gone" />
                          <RelativeLayout
                              android:layout_centerVertical="true"
                              android:layout_alignParentRight="true"
                              android:layout_marginRight="10dp"
                              android:layout_width="45dp"
                              android:layout_height="wrap_content">
                              <TextView
                                  android:id="@+id/tv_tag2_power"
                                  android:layout_width="wrap_content"
                                  android:layout_height="wrap_content"
                                  android:text="" />
                          </RelativeLayout>



                          <TextView
                              android:id="@+id/tv_num3_power"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_alignParentRight="true"
                              android:layout_marginRight="60dp"
                              android:layout_centerVertical="true"
                              android:text=""
                              android:textSize="@dimen/text_sixteen" />

                          <View
                              android:id="@+id/tag3_power_one"
                              android:layout_width="match_parent"
                              android:layout_height="1dp"
                              android:layout_marginTop="39dp"
                              android:background="@color/color_operate">

                          </View>
                      </RelativeLayout>

                      <RelativeLayout
                          android:layout_below="@+id/rl_power3"
                          android:id="@+id/rl_power4"
                          android:layout_width="match_parent"
                          android:layout_height="40dp">

                          <TextView
                              android:id="@+id/tv_power_one_tag4"
                              android:layout_width="150dp"
                              android:layout_height="wrap_content"
                              android:layout_centerVertical="true"
                              android:layout_marginLeft="@dimen/five"
                              android:layout_toRightOf="@+id/iv_power_one_tag4"
                              android:text=""
                              android:textSize="14sp" />


                          <ImageView
                              android:id="@+id/iv_power_one_tag4"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_centerVertical="true"
                              android:layout_marginLeft="@dimen/twelve"
                              android:src="@mipmap/shouyi"
                              android:visibility="gone" />
                          <RelativeLayout
                              android:layout_centerVertical="true"
                              android:layout_alignParentRight="true"
                              android:layout_marginRight="10dp"
                              android:layout_width="45dp"
                              android:layout_height="wrap_content">
                              <TextView
                                  android:id="@+id/tv_tag3_power"
                                  android:layout_width="wrap_content"
                                  android:layout_height="wrap_content"
                                  android:text="" />
                          </RelativeLayout>



                          <TextView
                              android:id="@+id/textView2"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_centerVertical="true"
                              android:layout_alignParentRight="true"
                              android:layout_marginRight="60dp"
                              android:text=""
                              android:textSize="@dimen/text_sixteen" />

                          <View
                              android:id="@+id/tag4_power_one"
                              android:layout_width="match_parent"
                              android:layout_height="1dp"
                              android:layout_marginTop="39dp"
                              android:background="@color/color_operate" />

                      </RelativeLayout>

                <RelativeLayout
                    android:layout_below="@+id/rl_power4"
                    android:id="@+id/rl_power5"
                    android:layout_width="match_parent"
                    android:layout_height="40dp">

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/five"
                        android:layout_toRightOf="@+id/iv_power_one_tag5"
                        android:text=""
                        android:textSize="14sp" />


                    <ImageView
                        android:id="@+id/iv_power_one_tag5"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/twelve"
                        android:src="@mipmap/shouyi"
                        android:visibility="gone" />

                    <RelativeLayout
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_width="45dp"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/tv_tag4_power"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="" />
                    </RelativeLayout>


                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="60dp"
                        android:text=""
                        android:textSize="@dimen/text_sixteen" />


                    <View
                        android:id="@+id/tag5_power_one"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="39dp"
                        android:background="@color/color_operate" />
                </RelativeLayout>


               <RelativeLayout
                   android:layout_below="@+id/rl_power5"
                   android:id="@+id/rl_power6"
                   android:layout_width="match_parent"
                   android:layout_height="40dp">

                   <TextView
                       android:id="@+id/tv_power_one_tag6"
                       android:layout_width="150dp"
                       android:layout_height="wrap_content"
                       android:layout_centerVertical="true"
                       android:layout_marginLeft="@dimen/five"
                       android:layout_toRightOf="@+id/iv_power_one_tag6"
                       android:text=""
                       android:textSize="14sp" />
                   <TextView
                       android:id="@+id/tv_power_one_tag6_1"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_centerVertical="true"
                       android:layout_marginLeft="@dimen/five"
                       android:layout_toRightOf="@+id/iv_power_one_tag6"
                       android:text=""
                       android:textSize="14sp" />
                   <TextView
                       android:layout_marginTop="21dp"
                       android:id="@+id/tv_power_one_tag6_2"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_toRightOf="@+id/tv_power_one_tag6_1"
                       android:text=""
                       android:textSize="10sp" />
                   <TextView
                       android:id="@+id/tv_power_one_tag6_3"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_centerVertical="true"
                       android:layout_toRightOf="@+id/tv_power_one_tag6_2"
                       android:text=""
                       android:textSize="14sp" />
                   <ImageView
                       android:id="@+id/iv_power_one_tag6"
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_centerVertical="true"
                       android:layout_marginLeft="@dimen/twelve"
                       android:src="@mipmap/jianpai"
                       android:visibility="gone" />

                   <RelativeLayout
                       android:layout_centerVertical="true"
                       android:layout_alignParentRight="true"
                       android:layout_marginRight="10dp"
                       android:layout_width="45dp"
                       android:layout_height="wrap_content">
                       <TextView
                           android:id="@+id/tv_tag5_power"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="" />
                   </RelativeLayout>


                   <TextView
                       android:id="@+id/tv_num6_power"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_centerVertical="true"
                       android:layout_alignParentRight="true"
                       android:layout_marginRight="60dp"
                       android:text=""
                       android:textSize="@dimen/text_sixteen" />

                   <View
                       android:layout_width="match_parent"
                       android:layout_height="1dp"
                       android:layout_marginTop="39dp"
                       android:background="@color/color_operate">
                   </View>


               </RelativeLayout>





            </RelativeLayout>
        </LinearLayout>


    </com.example.administrator.spms.refresh.PullableScrollView>

    <include layout="@layout/allview_load_more" />

</com.example.administrator.spms.refresh.PullToRefreshLayout>