                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:layout_centerVertical="true"
                         android:layout_alignParentRight="true"
                         android:layout_marginRight="80dp"
                         android:text=""
                         android:textSize="@dimen/text_sixteen" />

                     <View
                         android:id="@+id/tag3_power_one"
                         android:layout_width="match_parent"
                         android:layout_height="1dp"
                         android:layout_marginTop="39dp"
                         android:background="@color/color_operate">

                     </View>
                 </RelativeLayout>


               <RelativeLayout
                   android:id="@+id/power_rl4"
                   android:layout_below="@+id/power_rl3"
                   android:layout_width="match_parent"
                   android:layout_height="40dp">
                   <TextView
                       android:textSize="14sp"
                       android:id="@+id/tv_power_two_tag3"
                       android:layout_width="120dp"
                       android:layout_height="wrap_content"
                       android:layout_centerVertical="true"
                       android:layout_marginLeft="@dimen/five"
                       android:layout_toRightOf="@+id/iv_power_two_tag4"
                       android:text="" />


                   <ImageView
                       android:id="@+id/iv_power_two_tag4"
                       android:layout_width="30dp"
                       android:layout_height="30dp"
                       android:layout_centerVertical="true"
                       android:layout_marginLeft="@dimen/twelve"
                       android:src="@mipmap/rizhao"
                       android:visibility="gone" />
                   <RelativeLayout
                       android:layout_centerVertical="true"
                       android:layout_alignParentRight="true"
                       android:layout_marginRight="10dp"
                       android:layout_width="60dp"
                       android:layout_height="wrap_content">
                       <TextView
                           android:id="@+id/tv_tag3_power"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="" />
                   </RelativeLayout>




                   <TextView
                       android:id="@+id/textView2"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_centerVertical="true"
                       android:layout_alignParentRight="true"
                       android:layout_marginRight="80dp"
                       android:text=""
                       android:textSize="@dimen/text_sixteen" />


                   <View
                       android:id="@+id/tag4_power_one"
                       android:layout_width="match_parent"
                       android:layout_height="1dp"
                       android:layout_marginTop="39dp"
                       android:background="@color/color_operate">

                   </View>

               </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/power_rl5"
                    android:layout_below="@+id/power_rl4"
                    android:layout_width="match_parent"
                    android:layout_height="40dp">
                    <TextView
                        android:textSize="14sp"
                        android:id="@+id/textView"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/five"
                        android:layout_toRightOf="@+id/iv_power_two_tag5"
                        android:text="" />


                    <ImageView
                        android:id="@+id/iv_power_two_tag5"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/twelve"
                        android:src="@mipmap/rizhao"
                        android:visibility="gone" />

                    <RelativeLayout
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/tv_tag4_power"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="" />
                    </RelativeLayout>



                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="80dp"
                        android:layout_centerVertical="true"
                        android:text=""
                        android:textSize="@dimen/text_sixteen" />


                    <View
                        android:id="@+id/tag5_power_one"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="39dp"
                        android:background="@color/color_operate">

                    </View>

                </RelativeLayout>



                <RelativeLayout
                    android:layout_below="@+id/power_rl5"
                    android:id="@+id/power_rl6"
                    android:layout_width="match_parent"
                    android:layout_height="40dp">
                    <TextView
                        android:textSize="14sp"
                        android:id="@+id/tv_power_two_tag4"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/five"
                        android:layout_toRightOf="@+id/iv_power_two_tag6"
                        android:text="" />

                    <ImageView
                        android:id="@+id/iv_power_two_tag6"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                       android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/twelve"
                        android:src="@mipmap/huanjing"
                        android:visibility="gone" />

                    <RelativeLayout
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/tv_tag5_power"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="" />
                    </RelativeLayout>


                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="80dp"
                        android:text=""
                        android:textSize="@dimen/text_sixteen" />


                    <View
                        android:id="@+id/tag6_power_two"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="39dp"
                        android:background="@color/color_operate">

                    </View>
                </RelativeLayout>



                <RelativeLayout
                    android:layout_below="@+id/power_rl6"
                    android:id="@+id/power_rl7"
                    android:layout_width="match_parent"
                    android:layout_height="40dp">

                    <TextView
                        android:textSize="14sp"
                        android:id="@+id/tv_power_two_tag5"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/five"
                        android:layout_toRightOf="@+id/iv_power_two_tag7"
                        android:text="" />

                    <ImageView
                        android:id="@+id/iv_power_two_tag7"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/twelve"
                        android:src="@mipmap/zujian"
                        android:visibility="gone" />
                    <RelativeLayout
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/tv_tag6_power"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="" />
                    </RelativeLayout>


                    <TextView
                        android:id="@+id/tv_num_power_two5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="80dp"
                        android:text=""
                        android:textSize="@dimen/text_sixteen" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="39dp"
                        android:background="@color/color_operate">
                    </View>


                </RelativeLayout>



            </RelativeLayout>
        </LinearLayout>
    </com.example.administrator.spms.refresh.PullableScrollView>

    <include
        android:id="@+id/include"
        layout="@layout/allview_load_more" />

</com.example.administrator.spms.refresh.PullToRefreshLayout>